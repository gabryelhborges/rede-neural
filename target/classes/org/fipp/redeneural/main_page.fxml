<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="800.0" minHeight="800.0" minWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fipp.redeneural.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
          <Button onAction="#onChooseFileButtonClick" text="Selecionar arquivo treinamento" />
         <Button fx:id="bttIniciarTreinamento" mnemonicParsing="false" onAction="#onIniciarTreinamentoClick" text="Iniciar Treinamento" />
      </children>
   </HBox>
    <TableView fx:id="tableView">

    </TableView>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Button mnemonicParsing="false" onAction="#onSelecionarArquivoTestes" text="Selecionar arquivo testes" />
         <Button fx:id="bttIniciarTeste" mnemonicParsing="false" onAction="#onIniciarTestesClick" text="Iniciar Testes" />
      </children>
   </HBox>
   <TableView fx:id="tableViewTestes" />
</VBox>
