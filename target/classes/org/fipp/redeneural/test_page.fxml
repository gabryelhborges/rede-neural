<VBox alignment="TOP_CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefWidth="800.0" spacing="20.0" style="-fx-background-color: rgb(60, 60, 60) rgb(60, 60, 60);" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fipp.redeneural.TestPageController">
   <children>
      <Text fx:id="test" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="TESTE" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <HBox alignment="CENTER" spacing="30.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onChooseFileButtonClick" text="Carregar Arquivo" />
            <TextField fx:id="caminho_arquivo" disable="true" prefHeight="25.0" prefWidth="798.0" />
         </children>
      </HBox>
      <TableView fx:id="tableView" style="-fx-background-color: rgb(80,80,80);" />
      <HBox alignment="BOTTOM_RIGHT">
         <children>
            <Button fx:id="bttIniciarTournament" mnemonicParsing="false" onAction="#onIniciarTreinamentoClick" text="Iniciar Treinamento" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="30.0" right="30.0" />
